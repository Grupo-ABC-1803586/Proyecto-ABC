+        $query->bindParam("password_hash", $password_hash, PDO::PARAM_STR);
+        $query->bindParam("email", $email, PDO::PARAM_STR);
+        $result = $query->execute();
+
+        if ($result) {
+            echo '<p class="success">Your registration was successful!</p>';
